{
    "app-id" : "app.drey.Dialect.Devel",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "master",
    "sdk" : "org.gnome.Sdk",
    "command" : "dialect",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--device=dri",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--socket=pulseaudio",
        "--own-name=app.drey.Dialect.SearchProvider"
    ],
    "modules" : [
        "pypi-dependencies.json",
        {
            "name" : "dialect",
            "buildsystem" : "meson",
            "config-opts": ["-Dprofile=development"],
            "sources" : [
                {
                    "type" : "dir",
                    "path" : "../../."
                }
            ]
        }
    ]
}
